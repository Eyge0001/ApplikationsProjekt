<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Spelaigen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResultatsidaActivity">


    <Button
        android:id="@+id/spelaIgenknapp"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="171dp"
        android:background="@drawable/ovalknapp"
        android:shadowColor="@color/Black"
        android:text="Försök igen" />

    <Button
        android:id="@+id/tillbakastartsidaknapp"
        android:layout_width="153dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="83dp"
        android:background="@drawable/ovalknapp"
        android:shadowColor="@color/Black"
        android:text="Gå till startsidan" />

    <TextView
        android:id="@+id/Resultattext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="23dp"
        android:text="Ditt resultat blev:"
        android:textSize="30dp"
        android:textStyle="bold"
        android:textColor="@color/Black"/>

    <TextView
        android:id="@+id/totalafrågortext"
        android:layout_width="111dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
